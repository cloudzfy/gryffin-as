    <dependency>
      <groupId>net.sourceforge.jchardet</groupId>
      <artifactId>jchardet</artifactId>
      <version>1.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>rhino</groupId>
      <artifactId>js</artifactId>
      <version>1.7R3</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>activesoap</groupId>
      <artifactId>jaxb-xalan</artifactId>
      <version>1.5</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.jvoicexml</groupId>
      <artifactId>org.jvoicexml</artifactId>
      <version>0.7.6.EA</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.jvoicexml</groupId>
      <artifactId>org.jvoicexml.client</artifactId>
      <version>0.7.6.EA</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.jvoicexml</groupId>
      <artifactId>org.jvoicexml.xml</artifactId>
      <version>0.7.6.EA</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.mobicents.tools</groupId>
        <artifactId>maven-library-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>copy-dependencies</goal>
              <goal>generate-descriptor</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <library-name>gryffin-as-library</library-name>
          <library-vendor>org.cloudzfy</library-vendor>
          <library-version>1.0</library-version>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
